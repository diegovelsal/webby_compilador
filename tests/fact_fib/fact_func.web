program factorialFunc;

var
    n : int;

void factorial(n : int) [
    var
        i : int;
        result : int;
    {
        i = 1;
        result = 1;
        while (i < n) do {
            result = result * i;
            i = i + 1;
        };
        print("Factorial desde funciÃ³n:", result);
    }
];

main {
    n = 5;
    factorial(n);
}

end
